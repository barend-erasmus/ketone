<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="view-header">
                    <div class="pull-right text-right" style="line-height: 14px">
                        <small>Ketone<br>Users - Edit<br><span class="c-white">v.0.0.1</span></small>
                    </div>
                    <div class="header-icon">
                        <i class="pe page-header-icon pe-7s-menu"></i>
                    </div>
                    <div class="header-title">
                         <h3><a href="/clients">Clients</a> - <a href="/clients/edit?id={{client.id}}">{{client.name}}</a> - <a href="/users?clientId={{client.id}}">Users</a> - {{editUser.username}}</h3>
                        <small>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</small>
                    </div>
                </div>
                <hr>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <h4>Details</h4>
                <form>
                    <div class="form-group">
                        <label for="username">Username</label>
                        <input disabled type="text" class="form-control" id="username" placeholder="Username" value="{{editUser.username}}">
                    </div>
                    <div class="form-group">
                        <label for="emailAddress">Email Address</label>
                        <input type="text" class="form-control" id="emailAddress" placeholder="Email Address" value="{{editUser.emailAddress}}">
                    </div>
                    <button disabled type="submit" class="btn btn-default">Save</button>
                </form>
            </div>
        </div>
    </div>
</section>